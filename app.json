        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "debuggable": true,
      "package": "com.dm.appvehiculos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "63307cfa-e879-4042-b5fc-efb32d87df62"
      }
    }
  }
}
